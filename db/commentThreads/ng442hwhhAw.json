c": true}}, {"kind": "youtube#commentThread", "etag": "2sXU02DdSWiKOPTl-VT_voI_fz8", "id": "UghtY8m-VLTmjngCoAEC", "snippet": {"videoId": "ng442hwhhAw", "topLevelComment": {"kind": "youtube#comment", "etag": "A6xHgar3dmQOhcwxfCbjx0HxoDU", "id": "UghtY8m-VLTmjngCoAEC", "snippet": {"videoId": "ng442hwhhAw", "textDisplay": "This has always been one of my favorite ending themes, and one of my favorite songs in general. I always make it a point to play this masterpiece at least once a year.", "textOriginal": "This has always been one of my favorite ending themes, and one of my favorite songs in general. I always make it a point to play this masterpiece at least once a year.", "authorDisplayName": "Haley S.", "authorProfileImageUrl": "https://yt3.ggpht.com/a/AATXAJweJ_vzt7C4xZPeXcQ0DMABSvoHmedXu9XzyWAFgA=s48-c-k-c0xffffffff-no-rj-mo", "authorChannelUrl": "http://www.youtube.com/channel/UCBRQeAf56_izv5Z00sywsVQ", "authorChannelId": {"value": "UCBRQeAf56_izv5Z00sywsVQ"}, "canRate": true, "viewerRating": "none", "likeCount": 0, "publishedAt": "2015-04-13T02:21:17Z", "updatedAt": "2015-04-13T02:21:17Z"}}, "canReply": true, "totalReplyCount": 0, "isPublic": true}}, {"kind": "youtube#commentThread", "etag": "40GJDj6uiXA7WSF4VfxzvoEoFA4", "id": "UggOJngVtY5rP3gCoAEC", "snippet": {"videoId": "ng442hwhhAw", "topLevelComment": {"kind": "youtube#comment", "etag": "X_RDkSMmIQ0xqE_7P2lpk9KaURM", "id": "UggOJngVtY5rP3gCoAEC", "snippet": {"videoId": "ng442hwhhAw", "textDisplay": "Best game I've ever played in my life. Every year, I turn on my NES at least 5 times per year to finish this game.", "textOriginal": "Best game I've ever played in my life. Every year, I turn on my NES at least 5 times per year to finish this game.", "authorDisplayName": "Atomikko", "authorProfileImageUrl": "https://yt3.ggpht.com/a/AATXAJwRRPwWQqIdk1wHVCMAIp1aEQDM-rBtGUdtOJR0bA=s48-c-k-c0xffffffff-no-rj-mo", "authorChannelUrl": "http://www.youtube.com/channel/UCzYIE88xVxI3xUPwFsPSOQg", "authorChannelId": {"valu