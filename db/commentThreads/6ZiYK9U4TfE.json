[{"kind": "youtube#commentThread", "etag": "1E_1SDToLCbRAApZtXIAgIn1cdg", "id": "UgybXcI5ME97a4dLHQN4AaABAg", "snippet": {"videoId": "6ZiYK9U4TfE", "topLevelComment": {"kind": "youtube#comment", "etag": "PYWsVWVp9do5-P3SZ3dd3A9vfas", "id": "UgybXcI5ME97a4dLHQN4AaABAg", "snippet": {"videoId": "6ZiYK9U4TfE", "textDisplay": "Another one of the very few Super FX games that existed on the SNES. Dirt Trax FX was nowhere near as popular as Stunt Race FX, and certainly not as good, but I did have a good time with it back then. The only reason I rented it was because I thought it was some sort of sequel to Stunt Race FX, which of course is not the case (see Acclaim logo). There was a theme for each driver that would play while racing but funnily enough, most of them didn't really fit a racing game. It's the case with Spike's mellow theme, but it was always my favorite because of that.", "textOriginal": "Another one of the very few Super FX games that existed on the SNES. Dirt Trax FX was nowhere near as popular as Stunt Race FX, and certainly not as good, but I did have a good time with it back then. The only reason I rented it was because I thought it was some sort of sequel to Stunt Race FX, which of course is not the case (see Acclaim logo). There was a theme for each driver that would play while racing but funnily enough, most of them didn't really fit a racing game. It's the case with Spike's mellow theme, but it was always my favorite because of that.", "authorDisplayName": "SupraDarky", "authorProfileImageUrl": "https://yt3.ggpht.com/a/AATXAJz-WP37CKyRsSGpodj0KF2a556urv8yhxQQEuPB=s48-c-k-c0xffffffff-no-rj-mo", "authorChannelUrl": "http://www.youtube.com/channel/UC6iBH7Pmiinoe902-JqQ7aQ", "authorChannelId": {"value": "UC6iBH7Pmiinoe902-JqQ7aQ"}, "canRate": true, "viewerRating": "none", "likeCount": 12, "publishedAt": "2018-05-22T22:59:54Z", "updatedAt": "2018-05-22T23:00:28Z"}}, "canReply": true, "totalReplyCount": 0, "isPublic": true}}, {"kind": "youtube#commentThread